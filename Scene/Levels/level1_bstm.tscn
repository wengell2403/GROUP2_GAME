text = "Also, be sure to be not swayed away by negative emotions and fear. "

[node name="DialogText9" type="Node" parent="DialogInteraction"]
script = ExtResource("18_bwury")
text = "All you have to do is to believe in yourself and you will survive each semester. Got it?"

[node name="DialogText10" type="Node" parent="DialogInteraction"]
