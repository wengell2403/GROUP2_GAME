text = "Ah, a new student. So you chose IT huh..."

[node name="DialogText4" type="Node" parent="DialogInteraction"]
script = ExtResource("18_bwury")
text = "Many students have lost their minds choosing this path. "

[node name="DialogText5" type="Node" parent="DialogInteraction"]
script = ExtResource("18_bwury")
text = "I’m kidding. I was just trying to loosen you up. You seem nervous."

[node name="DialogText6" type="Node" parent="DialogInteraction"]
script = ExtResource("18_bwury")
text = "That’s the spirit! All college courses have their own difficulty and the challenges vary. "

[node name="DialogText7" type="Node" parent="DialogInteraction"]
